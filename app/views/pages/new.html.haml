= content_for :menu_link do
  = render 'menu_link'

%h2
  = @wiki_info.name
  &raquo;
  = t('terms.new_page')

= render 'form'


:coffeescript
  $ ->
    parse_md = ->
      result = Markdown $('#markdown-text').val()
      $('#preview-result').html result
    $('#markdown-text').keyup ->
      markdown_text = $('#markdown-text').val()
      if (prev != markdown_text)
        prev = markdown_text
        parse_md()
